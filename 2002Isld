// Landsat 5 image - 2002

// Define the date range and cloud cover threshold
var startDate = '2002-01-01';
var endDate = '2003-12-31';
var cloudCoverThreshold = 5;

// Define the region of interest (ROI) as a geometry (e.g., a bounding box)


// Load Landsat 5 Collection 1 Tier 1 TOA
var landsatCollection = ee.ImageCollection("LANDSAT/LT05/C02/T1_TOA")
  .filterBounds(roi)
  .filterDate(startDate, endDate)
  .filter(ee.Filter.calendarRange(5, 7, 'month'))
  .filter(ee.Filter.lt('CLOUD_COVER', cloudCoverThreshold))
  .median()
  .clip(roi);

print(landsatCollection);

// Define visualization parameters for Landsat image
var visParamsTrue = {
  bands: ['B3', 'B2', 'B1'], // Adjust bands as needed for Landsat 5
  min: 0.10491735249757767,
  max: 0.2481740215420723,
};

// Add Landsat image to the map
Map.addLayer(landsatCollection, visParamsTrue, "Landsat 5 Image");

// Center the map on the region of interest
Map.centerObject(roi, 10);

// Select relevant bands for classification
var bands = ['B1', 'B2', 'B3', 'B4', 'B5', 'B7']; // Adjust bands as needed for Landsat 5

// Load training data (assuming you have defined water, forest, vegetation, barren, and built_up)
var trainingData = water.merge(vegetation).merge(barren).merge(builtup);
print(trainingData);

// Define the classification bands and class labels
var classProperty = 'Class';
var classLabels = ['water', 'vegetation', 'barren', 'built_up'];
var input = landsatCollection.select(bands);

// Sample the training data from the imagery
var training = input.sampleRegions({
  collection: trainingData,
  properties: [classProperty],
  scale: 30
});

// Train a Naive Bayes classifier from the training sample.
var classifier = ee.Classifier.smileGradientTreeBoost(10, 0.005, 0.7, null, "LeastAbsoluteDeviation", 0).train(training, classProperty, bands);

// Get information about the trained classifier.
print('Results of trained classifier', classifier.explain());

// Classify the imagery
var classified = input.classify(classifier);

// Display the classified result on the map
Map.centerObject(roi, 10);
Map.addLayer(classified, {min: 0, max: 4, palette: ['blue', 'green', 'lime', 'yellow', 'red']});

// Accuracy Assessment
var trainingDataWithRandom = training.randomColumn('random'); // Add a random column
var trainSet = trainingDataWithRandom.filter(ee.Filter.lt('random', 0.8));
var testSet = trainingDataWithRandom.filter(ee.Filter.gte('random', 0.8));

// Classify the testing set and get a confusion matrix.
var confusionMatrix = testSet.classify(classifier)
  .errorMatrix({
    actual: 'Class', 
    predicted: 'classification'
  });

print('Confusion matrix:', confusionMatrix);
print('Overall Accuracy:', confusionMatrix.accuracy());
print('Producers Accuracy:', confusionMatrix.producersAccuracy());
print('Consumers Accuracy:', confusionMatrix.consumersAccuracy());
print('Kappa Statistics:', confusionMatrix.kappa());

// Export classified map to Google Drive
Export.image.toDrive({
  image: classified,
  description: 'GradientTreeBoost2002',
  scale: 30,
  region: roi,
  maxPixels: 1e13,
});
